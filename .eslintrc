{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "never"],
    "prefer-template": 0,
    "no-confusing-arrow": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "dot-notation": 0,
    "quote-props": 0,
    "max-len": 0,
    "react/no-multi-comp": 0,
    "react/no-did-update-set-state": 0,
    "react/prefer-stateless-function": 0,
    "react/sort-comp": 0
  }
}
